<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.slothnull.android.medox.fragment.WarehouseFragment">

    <TextView
        android:id="@+id/BillCountView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BillCount"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="13dp" />

    <TextView
        android:id="@+id/textBillCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/BillCountView"
        android:layout_centerHorizontal="true"
        android:text="Wait..."
        android:textSize="20sp" />

    <Button
        android:id="@+id/addData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/textBillCount"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="115dp"
        android:text="AddData" />

    <EditText
        android:id="@+id/textBillArray"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button11"
        android:layout_alignStart="@+id/BillCountView"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textBillArray"
        android:layout_alignStart="@+id/textBillArray"
        android:layout_marginBottom="62dp"
        android:text=" Added bill Array +/- split by ','"
        android:textSize="20sp"
        android:id="@+id/textView5" />

    <EditText
        android:id="@+id/drug1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textBillCount"
        android:layout_below="@+id/textBillArray"
        android:layout_marginTop="51dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name" />

    <EditText
        android:id="@+id/drug2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/drug1"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name" />

    <EditText
        android:id="@+id/drug3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/drug2"
        android:layout_below="@+id/drug2"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name" />

    <EditText
        android:id="@+id/drug4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/drug3"
        android:layout_below="@+id/drug3"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name" />

    <Button
        android:id="@+id/getNames"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/drug3"
        android:layout_marginBottom="16dp"
        android:layout_toEndOf="@+id/textView5"
        android:text="getNames"
        tools:text="getNames" />

    <Button
        android:id="@+id/updateNames"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button12"
        android:layout_below="@+id/drug3"
        android:layout_marginTop="17dp"
        android:text="updateNames" />

    <Button
        android:id="@+id/getBillCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button11"
        android:layout_below="@+id/textBillCount"
        android:text="getBillCount" />

</android.widget.RelativeLayout>
