<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.slothnull.android.medox.Schedule">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:onClick="addEntry"
        android:text="addEntry"
        tools:layout_editor_absoluteX="293dp"
        tools:layout_editor_absoluteY="456dp"
        android:id="@+id/button13" />

    <EditText
        android:id="@+id/textBillArray"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="billArray" />

    <EditText
        android:id="@+id/textTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button13"
        android:layout_alignEnd="@+id/textBillArray"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="time" />

    <ListView
        android:id="@+id/listSchedule"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/textTime"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />

</android.widget.RelativeLayout>
